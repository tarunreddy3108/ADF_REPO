{
	"name": "dataflow_aggregate",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "store_sales",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "store_sales",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Sale_ID as short,",
				"          Date as date,",
				"          Region as string,",
				"          City as string,",
				"          Category as string,",
				"          Item as string,",
				"          Quantity as short,",
				"          Price as double,",
				"          Total_Amount as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 aggregate(groupBy(Category),",
				"     count = count(Category)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Sale_ID as string,",
				"          Date as string,",
				"          Region as string,",
				"          City as string,",
				"          Category as string,",
				"          Item as string,",
				"          Quantity as string,",
				"          Price as string,",
				"          Total_Amount as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}